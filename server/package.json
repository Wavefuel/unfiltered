{
	"name": "wavefuel-unfiltered-server",
	"version": "0.1.0",
	"description": ".",
	"type": "commonjs",
	"scripts": {
		"encrypt-dev-env": "dotenvx encrypt -f .env.development",
		"encrypt-stg-env": "dotenvx encrypt -f .env.stage",
		"encrypt-prod-env": "dotenvx encrypt -f .env.production",
		"decrypt-dev-env": "dotenvx decrypt -f .env.development",
		"decrypt-stg-env": "dotenvx decrypt -f .env.stage",
		"decrypt-prod-env": "dotenvx decrypt -f .env.production",
		"clean": "rimraf compiled/*",
		"build-ts": "tsc -p ./config/tsconfig-cjs.json",
		"alias": "npx tsc-alias -p ./config/tsconfig-cjs.json --verbose",
		"post-compile": "copyfiles --all --up 1 --exclude \"./**/*.{ts,tsx}\" \"./src/**/*\" ./compiled/src",
		"compile": "npm-run-all -s clean build-ts post-compile alias",
		"generate-manifest": "node ./config/generateApiManifest.js",
		"rollup": "rimraf dist/* && rollup -c",
		"dist": "rimraf dist/* && node ./config/bundler.js && npm run post-dist",
		"post-dist": "copyfiles --all --up 1 --exclude \"./**/*.{ts,tsx}\" \"./src/**/*\" ./dist/src",
		"compose": "docker-compose up --force-recreate --build -d",
		"prune": "docker image prune -f",
		"profile": "node --inspect index.js",
		"siege": "siege -c 100 -r 1000 http://localhost:3000/",
		"kill": "kill -9 $(lsof -ti:3000)",
		"redis": "redis-stack-server",
		"redis-insight": "redisinsight",
		"dev": "dotenvx run -f .env -- npx nodemon --config nodemon.json",
		"create-plugin": "tsx ./cli/create-plugin.ts"
	},
	"main": "dist/index.js",
	"author": "Wavefuel",
	"license": "ISC",
